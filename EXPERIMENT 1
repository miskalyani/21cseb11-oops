package javaapplication11;

/**
 *
 * @author test02
 */
import java.util.*;
public class NewClass {
    public static void main(String[] args) {
        int i,j,rows;
        Scanner obj= new Scanner(System.in);
        System.out.println("Enter no. of Rows:");
        rows= obj.nextInt();
        System.out.println("This is the pattern of "+rows+" Rows");
        for(i=1;i<=rows;i++)
        {
            for(j=rows;j>=i;j--)
            {
                System.out.print(j+" ");
            }
        System.out.println();
        }
    }
    
}
package javaapplication11;

