package javaapplication2;

import java.util.*;
public class Sample1 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        int lowerLimit = 101; // Starting from 101 to exclude 100
        int upperLimit = 200;
        int divisor = 7;
        int sum = 0;

        for (int num = lowerLimit; num < upperLimit; num++) {
            if (num % divisor == 0) {
                sum += num;
            }
        }
        // TODO code application logic here
        System.out.println("Sum of numbers divisible by " + divisor + " between " + lowerLimit + " and " + upperLimit + " is: " + sum);
    }
   
}
package javaapplication11;

/**
 *
 * @author test02
 */
import java.util.*;
public class NewClass2 {
    public static void main(String[] args){
        int l,h,d,i,s=0;
        Scanner obj= new Scanner(System.in);
        System.out.println("Enter lower number :");
        l= obj.nextInt();
        System.out.println("Enter upper number :");
        h= obj.nextInt();
        System.out.println("Enter divisor :");
        d= obj.nextInt();
        System.out.println("Numbers between "+l+" and "+h+" divided by "+ d);
        for(i=l;i<=h;i++)
        {
            if(i%d==0){
                System.out.print(i+" ");
                s+=i;
            }
        }
        System.out.println();
        System.out.println("The sum is: "+s);
    }
    
}

